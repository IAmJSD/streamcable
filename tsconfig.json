{
    "buildOptions": {
        "outDir": "dist",
        "declaration": true,
        "sourceMap": true,
        "module": "ESNext",
        "target": "ES2020",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2020"
    }
}
